      ],
      "cache-plugin": [
        "./types/plugins/cache/mod.d.ts"
      ],
      "fileloader-plugin": [
        "./types/plugins/fileloader/mod.d.ts"
      ],
      "helpers-plugin": [
        "./types/plugins/helpers/mod.d.ts"
      ],
      "permissions-plugin": [
        "./types/plugins/permissions/mod.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./esm/mod.js",
      "require": "./script/mod.js",
      "types": "./types/mod.d.ts"
    },
    "./rest": {
      "import": "./esm/rest/mod.js",
      "require": "./script/rest/mod.js",
      "types": "./types/rest/mod.d.ts"
    },
    "./gateway": {
      "import": "./esm/gateway/mod.js",
      "require": "./script/gateway/mod.js",
      "types": "./types/gateway/mod.d.ts"
